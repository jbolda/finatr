{
  "bundle": {
    "active": true,
    "targets": "all",
    "windows": {
      "certificateThumbprint": null,
      "digestAlgorithm": "sha256",
      "timestampUrl": ""
    },
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [],
    "externalBin": [],
    "copyright": "",
    "category": "DeveloperTool",
    "shortDescription": "",
    "longDescription": "",
    "macOS": {
      "frameworks": [],
      "minimumSystemVersion": "",
      "exceptionDomain": "",
      "signingIdentity": null,
      "entitlements": null
    },
    "linux": {
      "deb": {
        "depends": []
      }
    },
    "createUpdaterArtifacts": "v1Compatible"
  },
  "build": {
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist",
    "beforeDevCommand": "npm run start",
    "devUrl": "http://localhost:1234"
  },
  "productName": "finatr",
  "identifier": "com.finatr.app",
  "plugins": {},
  "app": {
    "security": {
      "csp": "default-src https: tauri: 'self'; connect-src ipc: http://ipc.localhost"
    },
    "windows": [
      {
        "title": "finatr",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false,
        "decorations": true,
        "transparent": true
      }
    ]
  }
}